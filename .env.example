# OpenRouter API (used by both tools for AI filtering)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Clado API (LinkedIn Enricher only - for extracting reactions and enriching contacts)
CLADO_API_KEY=your_clado_api_key_here

# Apollo API (LinkedIn Enricher only - alternative email enrichment provider)
APOLLO_API_KEY=your_apollo_api_key_here

# App URL (optional, defaults to http://localhost:3000)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Notion API (for reading/writing to Notion databases)
NOTION_API_KEY=your_notion_integration_token_here

# Growth Dashboard APIs
INSTANTLY_API_KEY=your_instantly_api_key_here
PROSP_API_KEY=your_prosp_api_key_here
DRIPPI_API_KEY=your_drippi_api_key_here

# Daily Lead Pipeline APIs
APIFY_API_KEY=your_apify_api_key_here
EXA_API_KEY=your_exa_api_key_here
ICYPEAS_API_KEY=your_icypeas_api_key_here
ICYPEAS_USER_ID=your_icypeas_user_id_here
